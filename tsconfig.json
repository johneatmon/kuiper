{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@components/*": ["src/components/*"],
      "@img/*": ["src/img/*"],
      "@layouts/*": ["src/layouts/*"],
      "@lib/*": ["src/lib/*"],
      "@styles/*": ["src/styles/*"]
    },
    "noEmit": true,
    "incremental": true,
    "jsx": "preserve",
    "jsxImportSource": "react",
    "types": ["@astrojs/image/client"]
  }
}
