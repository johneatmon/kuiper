{
  "name": "@johneatmon/kuiper",
  "type": "module",
  "private": true,
  "scripts": {
    "format": "run-s format:*",
    "format:prettier": "prettier -w --plugin-search-dir=. .",
    "format:eslint": "eslint --fix \"**/*.{js,ts,jsx,tsx,mjs,cjs,astro}\"",
    "lint": "run-p --print-label lint:*",
    "lint:prettier": "prettier --check --plugin-search-dir=. .",
    "lint:eslint": "eslint \"**/*.{js,ts,jsx,tsx,mjs,cjs,astro}\"",
    "lint:typescript": "tsc --noEmit",
    "lint:astro": "cd src && astro check",
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro"
  },
  "dependencies": {
    "@astrojs/image": "^0.14.0",
    "@astrojs/mdx": "^0.16.0",
    "@astrojs/prefetch": "^0.1.2",
    "@astrojs/react": "^2.0.2",
    "@astrojs/sitemap": "^1.0.1",
    "@astrojs/tailwind": "^3.0.1",
    "astro": "^2.0.6",
    "fontaine": "^0.2.3",
    "mdast-util-to-string": "^3.1.1",
    "postcss": "^8.4.21",
    "postcss-nesting": "^11.1.0",
    "postcss-preset-env": "^8.0.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "reading-time": "^1.5.0",
    "sharp": "^0.31.3",
    "smartypants": "^0.1.6",
    "tailwindcss": "^3.0.24",
    "unist-util-visit": "^4.1.2"
  },
  "devDependencies": {
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.32.0",
    "eslint-plugin-astro": "^0.23.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.3",
    "prettier-plugin-astro": "^0.8.0",
    "prettier-plugin-tailwindcss": "^0.2.2"
  }
}
